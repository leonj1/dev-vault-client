<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>json,lcov,opencover,cobertura</Format>
          <Include>[CrossPlatformApp.*]*</Include>
